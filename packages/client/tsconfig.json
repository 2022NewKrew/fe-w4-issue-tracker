{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "rootDir": ".",
    "outDir": "./dist",
    "composite": true,
    "allowJs": false,
    "declaration": true,
    "declarationMap": true,
    "allowSyntheticDefaultImports": true,
    "jsxImportSource": "@emotion/react",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@hooks": ["./src/hooks"],
      "@assets/*": ["./src/assets/*"],
      "@icon": ["./src/components/common/Icon.tsx"],
      "@pages/*": ["./src/pages/*"],
      "@services/*": ["./src/services/*"],
      "@stores/*": ["./src/stores/*"],
      "@styles/*": ["./src/styles/*"],
      "@utils/*": ["./src/utils/*"],
      "@interface/*": ["./src/interface/*"]
    }
  },
  //   "references": [
  //     {
  //       "path": "../server"
  //     }
  //   ],
  "include": ["@types", "src/**/*"],
  "exclude": ["node_modules", "dist", "*.d.ts"]
}
